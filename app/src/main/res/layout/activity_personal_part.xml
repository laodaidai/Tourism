<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        tools:context=".PersonalActivity"
        tools:ignore="MissingConstraints">

        <com.baoyz.widget.PullRefreshLayout
            android:id="@+id/prl_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp">

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:gravity="center|left"
                        android:background="@color/color_white">


                        <ImageView
                            android:id="@+id/personal_Shopping_Cart_Img"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="20dp"
                            android:layout_gravity="center|left"
                            android:src="@drawable/personal_shop_car" />
                        <TextView
                            android:id="@+id/personal_Shopping_Cart_Text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="70dp"
                            android:layout_gravity="center|left"
                            android:text="购物车"
                            android:textSize="12sp"
                            android:textColor="@color/color_black"/>

                        <ImageView
                            android:id="@+id/persongal_shopping_arrow"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp"
                            android:layout_gravity="center|right"
                            android:src="@drawable/personal_arrow_white_right" />
                    </FrameLayout>

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="10dp"
                        android:background="@color/color_white">
                        <ImageView
                            android:id="@+id/personal_Holiday_order_Img"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="20dp"
                            android:layout_gravity="left|center"
                            android:src="@drawable/personal_order" />
                        <TextView
                            android:id="@+id/personal_Holiday_order_Text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="70dp"
                            android:layout_gravity="center|left"
                            android:text="度假全部订单"
                            android:textSize="12sp"
                            android:textColor="@color/color_black"/>

                        <ImageView
                            android:id="@+id/persongal_Holiday_arrow"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp"
                            android:layout_gravity="center|right"
                            android:src="@drawable/personal_arrow_white_right" />
                    </FrameLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:layout_marginTop="1dp"
                        android:background="@color/color_white"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_gravity="center"
                                android:layout_marginTop="15dp"
                                android:src="@drawable/personal_payment"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="5dp"
                                android:textColor="@color/color_black"
                                android:textSize="12sp"
                                android:text="待付款"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_gravity="center"
                                android:layout_marginTop="15dp"
                                android:src="@drawable/personal_consumption"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="5dp"
                                android:textColor="@color/color_black"
                                android:textSize="12sp"
                                android:text="待消费"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_gravity="center"
                                android:layout_marginTop="15dp"
                                android:src="@drawable/personal_evaluate"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="5dp"
                                android:textColor="@color/color_black"
                                android:textSize="12sp"
                                android:text="待评价"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_gravity="center"
                                android:layout_marginTop="15dp"
                                android:src="@drawable/personal_refund"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="5dp"
                                android:textColor="@color/color_black"
                                android:textSize="12sp"
                                android:text="退款中"/>
                        </LinearLayout>
                    </LinearLayout>

                    <FrameLayout
                        android:id="@+id/btn_coupon"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center|left"
                        android:background="@color/color_white">
                        <ImageView
                            android:id="@+id/personal_Coupon_Img"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="20dp"
                            android:layout_gravity="center|left"
                            android:src="@drawable/personal_coupon" />
                        <TextView
                            android:id="@+id/personal_Coupon_Text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="70dp"
                            android:layout_gravity="center|left"
                            android:text="我的优惠卷"
                            android:textSize="12sp"
                            android:textColor="@color/color_black"/>

                        <ImageView
                            android:id="@+id/persongal_Coupon_arrow"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp"
                            android:layout_gravity="center|right"
                            android:src="@drawable/personal_arrow_white_right" />

                    </FrameLayout>

                    <include layout="@layout/activity_line_not_full"/>

                    <FrameLayout
                        android:id="@+id/btn_mycollection"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:background="@color/color_white"
                        android:gravity="center|left"
                        android:onClick="my_collection">

                        <ImageView
                            android:id="@+id/personal_Collection_Img"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_gravity="center|left"
                            android:layout_marginLeft="20dp"
                            android:src="@drawable/personal_collection" />

                        <TextView
                            android:id="@+id/personal_Collection_Text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|left"
                            android:layout_marginLeft="70dp"
                            android:text="我的收藏"
                            android:textColor="@color/color_black"
                            android:textSize="12sp" />

                        <ImageView
                            android:id="@+id/persongal_Collection_arrow"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|right"
                            android:layout_marginRight="10dp"
                            android:src="@drawable/personal_arrow_white_right" />
                    </FrameLayout>

                    <include layout="@layout/activity_line_not_full"/>

                    <FrameLayout
                        android:id="@+id/btn_mysubscriptions"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:gravity="center|left"
                        android:onClick="my_subscriptions"
                        android:background="@color/color_white">
                        <ImageView
                            android:id="@+id/personal_Subscribe_Img"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="20dp"
                            android:layout_gravity="center|left"
                            android:src="@drawable/personal_subscribe" />
                        <TextView
                            android:id="@+id/personal_Subscribe_Text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="70dp"
                            android:layout_gravity="center|left"
                            android:text="我的订阅"
                            android:textSize="12sp"
                            android:textColor="@color/color_black"/>

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp"
                            android:layout_gravity="center|right"
                            android:src="@drawable/personal_arrow_white_right" />

                    </FrameLayout>
                    <include layout="@layout/activity_line_not_full"/>
                    <FrameLayout
                        android:id="@+id/btn_member"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:gravity="center|left"
                        android:onClick="my_subscriptions"
                        android:background="@color/color_white">
                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="20dp"
                            android:layout_gravity="center|left"
                            android:src="@drawable/personal_member_recharge" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="70dp"
                            android:layout_gravity="center|left"
                            android:text="会员充值"
                            android:textSize="12sp"
                            android:textColor="@color/color_black"/>

                        <ImageView
                            android:id="@+id/persongal_Subscribe_arrow"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp"
                            android:layout_gravity="center|right"
                            android:src="@drawable/personal_arrow_white_right" />

                    </FrameLayout>
                    <include layout="@layout/activity_line_not_full"/>

                    <FrameLayout
                        android:id="@+id/btn_holidayprbolem"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:gravity="center|left"
                        android:layout_marginTop="5dp"
                        android:onClick="my_holiday_prbolem"
                        android:background="@color/color_white">

                        <ImageView
                            android:id="@+id/personal_Feedback_Img"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="20dp"
                            android:layout_gravity="center|left"
                            android:src="@drawable/personal_feedback" />
                        <TextView
                            android:id="@+id/personal_Feedback_Text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="70dp"
                            android:layout_gravity="center|left"
                            android:text="意见反馈"
                            android:textSize="12sp"
                            android:textColor="@color/color_black"/>

                        <ImageView
                            android:id="@+id/persongal_Feedback_arrow"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp"
                            android:layout_gravity="center|right"
                            android:src="@drawable/personal_arrow_white_right" />
                    </FrameLayout>
                    <include layout="@layout/activity_line_not_full"/>

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:background="#00000000">

                    </FrameLayout>
                </LinearLayout>
            </ScrollView>
        </com.baoyz.widget.PullRefreshLayout>
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>