<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
   <FrameLayout
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       tools:ignore="MissingConstraints">
       <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:orientation="vertical">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@color/color_white">
        <ImageView
            android:id="@+id/btn_return_arrow"
            android:layout_width="20dp"
            android:layout_height="20sp"
            android:layout_marginLeft="10dp"
            android:layout_gravity="center|left"
            android:onClick="my_holiday_problem_return_arrow"
            android:src="@drawable/personal_blue_left"
            tools:ignore="OnClick" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="返回"
            android:layout_gravity="center|left"
            android:layout_marginLeft="40dp"
            android:textSize="15sp"
            android:textColor="#04C0B7"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="度假问题"
            android:textSize="20sp"
            android:textColor="@color/color_black"/>
    </FrameLayout>
           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="100dp"
               android:layout_marginTop="1dp"
               android:orientation="horizontal">

               <LinearLayout
                   android:layout_width="fill_parent"
                   android:layout_height="100dp"
                   android:orientation="vertical"
                   android:layout_marginLeft="1dp"
                   android:layout_weight="1"
                   android:background="@color/color_white">
                   <ImageView
                       android:layout_width="40dp"
                       android:layout_height="40dp"
                       android:layout_marginTop="15dp"
                       android:layout_gravity="center"
                       android:src="@drawable/personal_insurance_problem"/>
                   <TextView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:layout_gravity="center"
                       android:layout_marginTop="10dp"
                       android:text="保险问题"
                       android:textSize="15sp"
                       android:textColor="@color/color_gray"/>

               </LinearLayout>
               <LinearLayout
                   android:layout_width="fill_parent"
                   android:layout_height="100dp"
                   android:orientation="vertical"
                   android:layout_marginLeft="1dp"
                   android:layout_weight="1"
                   android:background="@color/color_white">
                   <ImageView
                       android:layout_width="40dp"
                       android:layout_height="40dp"
                       android:layout_marginTop="15dp"
                       android:layout_gravity="center"
                       android:src="@drawable/personal_predetermined_problem"/>
                   <TextView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:layout_gravity="center"
                       android:layout_marginTop="10dp"
                       android:text="预定问题"
                       android:textSize="15sp"
                       android:textColor="@color/color_gray"/>

               </LinearLayout>

               <LinearLayout
                   android:layout_width="fill_parent"
                   android:layout_height="100dp"
                   android:orientation="vertical"
                   android:background="@color/color_white"
                   android:layout_marginLeft="1dp"
                   android:layout_weight="1">
                   <ImageView
                       android:layout_width="40dp"
                       android:layout_height="40dp"
                       android:layout_marginTop="15dp"
                       android:layout_gravity="center"
                       android:src="@drawable/personal_travel_problem"/>
                   <TextView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:layout_gravity="center"
                       android:layout_marginTop="10dp"
                       android:text="出行问题"
                       android:textSize="15sp"
                       android:textColor="@color/color_gray"/>

               </LinearLayout>
               <LinearLayout
                   android:layout_width="fill_parent"
                   android:layout_height="100dp"
                   android:orientation="vertical"
                   android:layout_marginLeft="1dp"
                   android:layout_weight="1"
                   android:background="@color/color_white">
                   <ImageView
                       android:layout_width="40dp"
                       android:layout_height="40dp"
                       android:layout_marginTop="15dp"
                       android:layout_gravity="center"
                       android:src="@drawable/personal_refund_invoice"/>
                   <TextView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:layout_gravity="center"
                       android:layout_marginTop="10dp"
                       android:text="退款发票"
                       android:textSize="15sp"
                       android:textColor="@color/color_gray"/>

               </LinearLayout>
           </LinearLayout>

           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="50dp"
               android:background="@color/color_white"
               android:orientation="horizontal"
               android:layout_marginTop="20dp">
               <ImageView
                   android:layout_width="12dp"
                   android:layout_height="12dp"
                   android:layout_gravity="center|left"
                   android:layout_marginLeft="15dp"
                   android:src="@drawable/personal_dot_bj"/>
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginLeft="10dp"
                   android:text="度假产品供应商一直未确定订单"
                   android:textSize="18sp"
                   android:textColor="@color/color_black"
                   android:layout_gravity="center"/>
               <FrameLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">
                   <ImageView
                       android:layout_width="30dp"
                       android:layout_height="30dp"
                       android:layout_gravity="center|right"
                       android:src="@drawable/personal_arrow_black_right"/>
               </FrameLayout>
           </LinearLayout>

           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="50dp"
               android:background="@color/color_white"
               android:orientation="horizontal"
               android:layout_marginTop="1dp">
               <ImageView
                   android:layout_width="12dp"
                   android:layout_height="12dp"
                   android:layout_gravity="center|left"
                   android:layout_marginLeft="15dp"
                   android:src="@drawable/personal_dot_bj"/>
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginLeft="10dp"
                   android:text="我的度假订单无法支付"
                   android:textSize="18sp"
                   android:textColor="@color/color_black"
                   android:layout_gravity="center"/>
               <FrameLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">
                   <ImageView
                       android:layout_width="30dp"
                       android:layout_height="30dp"
                       android:layout_gravity="center|right"
                       android:src="@drawable/personal_arrow_black_right"/>
               </FrameLayout>
           </LinearLayout>
           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="50dp"
               android:background="@color/color_white"
               android:orientation="horizontal"
               android:layout_marginTop="1dp">
               <ImageView
                   android:layout_width="12dp"
                   android:layout_height="12dp"
                   android:layout_gravity="center|left"
                   android:layout_marginLeft="15dp"
                   android:src="@drawable/personal_dot_bj"/>
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginLeft="10dp"
                   android:text="如何变更出行时间"
                   android:textSize="18sp"
                   android:textColor="@color/color_black"
                   android:layout_gravity="center"/>
               <FrameLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">
                   <ImageView
                       android:layout_width="30dp"
                       android:layout_height="30dp"
                       android:layout_gravity="center|right"
                       android:src="@drawable/personal_arrow_black_right"/>
               </FrameLayout>
           </LinearLayout>
           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="50dp"
               android:background="@color/color_white"
               android:orientation="horizontal"
               android:layout_marginTop="1dp">
               <ImageView
                   android:layout_width="12dp"
                   android:layout_height="12dp"
                   android:layout_gravity="center|left"
                   android:layout_marginLeft="15dp"
                   android:src="@drawable/personal_dot_bj"/>
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginLeft="10dp"
                   android:text="度假产品保险能退吗"
                   android:textSize="18sp"
                   android:textColor="@color/color_black"
                   android:layout_gravity="center"/>
               <FrameLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">
                   <ImageView
                       android:layout_width="30dp"
                       android:layout_height="30dp"
                       android:layout_gravity="center|right"
                       android:src="@drawable/personal_arrow_black_right"/>
               </FrameLayout>
           </LinearLayout>
           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="50dp"
               android:background="@color/color_white"
               android:orientation="horizontal"
               android:layout_marginTop="1dp">
               <ImageView
                   android:layout_width="12dp"
                   android:layout_height="12dp"
                   android:layout_gravity="center|left"
                   android:layout_marginLeft="15dp"
                   android:src="@drawable/personal_dot_bj"/>
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginLeft="10dp"
                   android:text="未收到消费码"
                   android:textSize="18sp"
                   android:textColor="@color/color_black"
                   android:layout_gravity="center"/>
               <FrameLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">
                   <ImageView
                       android:layout_width="30dp"
                       android:layout_height="30dp"
                       android:layout_gravity="center|right"
                       android:src="@drawable/personal_arrow_black_right"/>
               </FrameLayout>
           </LinearLayout>
           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="50dp"
               android:background="@color/color_white"
               android:orientation="horizontal"
               android:layout_marginTop="1dp">
               <ImageView
                   android:layout_width="12dp"
                   android:layout_height="12dp"
                   android:layout_gravity="center|left"
                   android:layout_marginLeft="15dp"
                   android:src="@drawable/personal_dot_bj"/>
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginLeft="10dp"
                   android:text="我要退度假产品"
                   android:textSize="18sp"
                   android:textColor="@color/color_black"
                   android:layout_gravity="center"/>
               <FrameLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">
                   <ImageView
                       android:layout_width="30dp"
                       android:layout_height="30dp"
                       android:layout_gravity="center|right"
                       android:src="@drawable/personal_arrow_black_right"/>
               </FrameLayout>
           </LinearLayout>

       </LinearLayout>
   </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>